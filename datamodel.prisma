enum Role {
  Member
  Admin
}

enum userStatus {
  Available
  Focus_mode
  Unavailable
}

type User {
  id: ID! @id
  email: String! @unique
  password: String!
  profile: Profile @relation(link: INLINE)
}

type Profile {
  id: ID! @id
  name: String!
  country: String
  timezone: String!
  role: Role! @default(value: Member)
  status: userStatus! @default(value: Unavailable)
  work_start_time: Int
  work_end_time: Int
}
